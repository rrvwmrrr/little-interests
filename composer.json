{
    "name": "rrvwmrrr/little-interests",
    "description": "little-interests",
    "keywords": [
        "rrvwmrrr",
        "little-interests"
    ],
    "homepage": "https://github.com/rrvwmrrr/little-interests",
    "license": "MIT",
    "authors": [
        {
            "name": "James Sessford",
            "email": "hello@jamessessford.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Rrvwmrrr\\Interests\\": "src",
            "Rrvwmrrr\\Interests\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rrvwmrrr\\Interests\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "XDEBUG_MODE=coverage vendor/bin/phpunit --colors=always",
        "test-coverage": "XDEBUG_MODE=coverage vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rrvwmrrr\\Interests\\InterestsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
